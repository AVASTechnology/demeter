{
    "name": "avastechnology/demeter",
    "version": "1.0.0",
    "description": "OOP approach for structural code generation.",
    "keywords": ["generation", "code style", "formatting"],
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony Green",
            "email": "packages@avastechnology.com"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "avastechnology/iolaus": "*",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "AVASTech\\Demeter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AVASTech\\Demeter\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
